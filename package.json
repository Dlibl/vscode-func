{
    "name": "func-vscode",
    "displayName": "func-vscode",
    "description": "",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.32.0"
    },
    "activationEvents": [
        "*"
    ],
    "categories": [
        "Programming Languages"
    ],
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "lint": "eslint . --ext .ts,.tsx",
        "watch": "tsc -watch -p ./"
    },
    "main": "out/extension.js",
    "contributes": {
        "languages": [
            {
                "id": "func",
                "aliases": [
                    "FunC",
                    "func"
                ],
                "extensions": [
                    ".fc",
                    ".func"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "func",
                "scopeName": "source.func",
                "path": "./syntaxes/.tmLanguage.json"
            }
        ]
    },
    "devDependencies": {
        "typescript": "^4.6.3",
        "vscode": "^1.1.37"
    }
}
